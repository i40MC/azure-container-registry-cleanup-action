{
  "name": "@i40mc/azure-container-registry-cleanup-action",
  "version": "1.0.1",
  "private": true,
  "description": "This action cleans up old image tags in an Azure Container Registry",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest",
    "all": "npm run build && npm run lint && npm run package",
    "start": "npm run build && node lib/main.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/actions/typescript-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.6.0",
    "@azure/container-registry": "^1.0.0",
    "@azure/identity": "^2.0.4"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.21",
    "@typescript-eslint/parser": "^5.13.0",
    "@vercel/ncc": "^0.33.3",
    "eslint": "^8.10.0",
    "eslint-plugin-github": "^4.3.5",
    "eslint-plugin-jest": "^26.1.1",
    "jest": "^27.5.1",
    "jest-circus": "^27.5.1",
    "js-yaml": "^4.1.0",
    "prettier": "2.5.1",
    "ts-jest": "^27.1.3",
    "typescript": "^4.6.2"
  }
}
